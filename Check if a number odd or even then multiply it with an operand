import java.util.Scanner;
public class oddeve
{
    public static void main(String args[])
    {
        Scanner sc= new Scanner(System.in);
        int a=sc.nextInt();
        if(a%2==0)
        {
            System.out.println(a*8);
        }
        else
        {
             System.out.println(a*9);
        }
        
    }
}
