import java.util.Scanner;
public class Flash
{
    public static void main(String args[])
    {
        int x1,x2,y1,y2;
      Scanner sc= new Scanner(System.in);
       x1=sc.nextInt();
       y1=sc.nextInt();
       x2=sc.nextInt();
       y2=sc.nextInt();
       float a=(x1+x2)/2;
       float b=(float)(y1+y2)/2;
       System.out.printf("%.1f\n",a);
       System.out.printf("%.1f",b);
      
    }
}
