import java.util.*;
public class Football
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a,b,c,d,e;
        a=sc.nextInt();
        b=sc.nextInt();
        c=a/b;
        d=b*c;
        e=a-d;
        System.out.println(c);
        System.out.println(e);
        
    }
}
