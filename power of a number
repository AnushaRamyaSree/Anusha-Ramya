import java.util.Scanner;
public class Pow
{
    public static void main(String args[])
    {
        Scanner sc= new Scanner(System.in);
        int base= sc.nextInt();
        int exp = sc.nextInt();
        long result=1;
        while(exp != 0)
        {
            result *= base;
            --exp;
        }
        System.out.println(result);
}

}
