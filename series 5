import java.util.*;
public class Series5
{
    public static void main(String[] args)
    {
        Scanner sc= new Scanner(System.in);
        int n=sc.nextInt();
        int a=11,b;
        for(int i=1;i<=n;i++)
        {
          b=a*a;
          System.out.println(b+" ");
          a=a+4;
        }
        
    }
}
