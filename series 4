import java.util.Scanner;
public class Series4
{
    public static void main(String[] args)
    {
        Scanner sc= new Scanner(System.in);
        float n=sc.nextFloat();
        float a=1,b;
        System.out.print(a/2);
        for(int i=1;i<n;i++)
        {
            a=a*3;
            b=a/2;
            System.out.printf("\t%.1f",b);
        }
    }
}
