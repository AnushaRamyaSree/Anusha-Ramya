import java.util.Scanner;
public class Garden
{
    public static void main(String args[])
    {
      Scanner sc= new Scanner(System.in);
      int x= sc.nextInt();
      int y= sc.nextInt();
      int z= sc.nextInt();
      if(z<=1 && z>=y || z%y==0 ||z%y==1 )
      {
          System.out.println("yes");
      }
      else
      {
          System.out.println("no");
      }
    }
}
