import java.util.Scanner;
public class Reverse
{
    public static void main(String[] args)
    {
        int num,reminder,reverse=0;
     Scanner sc= new Scanner(System.in);
     num=sc.nextInt();
     while(num>0)
     {
         reminder=num%10;
         reverse=reverse*10+reminder;
         num=num/10;
     }
     System.out.println(reverse);
    }
}
