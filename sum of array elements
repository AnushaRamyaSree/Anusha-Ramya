import java.util.Scanner;
public class Sumarray
{
    public static void main(String[] args)
    {
        Scanner sc= new Scanner(System.in);
        int n=sc.nextInt();
        int a[]=new int[n];
       int large=0;
       for(int i=0;i<n;i++)
       {
         a[i]=sc.nextInt();
       }
       for(int i=0;i<n;i++)
       {
           large=large+a[i];
       }
       System.out.println(large);
    }
}
